{
  "name": "@autotelic/redux-core-reducers",
  "version": "0.1.0",
  "description": "A collection of re-usable and configurable reducers redux applications",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "flow": "flow",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:watch": "watch 'npm run build' ./src/",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive ./test"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "keywords": [],
  "author": "Autotelic",
  "repository": "https://github.com/autotelic/redux-core-reducers.git",
  "bugs": {
    "url": "https://github.com/autotelic/redux-core-reducers.git/issues"
  },
  "homepage": "https://github.com/autotelic/redux-core-reducers.git",
  "license": "MIT",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "flow-bin": "^0.86.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.5.2",
    "sinon": "^7.1.1",
    "watch": "^0.17.1"
  },
  "npmName": "redux-core-reducers",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ]
}
