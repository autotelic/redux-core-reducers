{
  "name": "@autotelic/redux-core-reducers",
  "version": "0.1.3",
  "description": "A collection of re-usable and configurable reducers for redux applications",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "flow": "flow",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:watch": "watch 'npm run build' ./src/",
    "prepublish": "npm run build",
    "test": "mocha --require @babel/register './src/**/*.test.js' --recursive",
    "commit": "git-cz"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-flow"
    ]
  },
  "keywords": [],
  "author": "Autotelic",
  "repository": "https://github.com/autotelic/redux-core-reducers.git",
  "bugs": {
    "url": "https://github.com/autotelic/redux-core-reducers.git/issues"
  },
  "homepage": "https://github.com/autotelic/redux-core-reducers.git",
  "license": "MIT",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.14.0",
    "flow-bin": "^0.126.1",
    "husky": "^4.0.5",
    "jsdoc": "^3.6.3",
    "mocha": "^9.0.0",
    "rimraf": "^3.0.1",
    "semantic-release": "^17.0.4",
    "sinon": "^9.0.0",
    "watch": "^1.0.2"
  },
  "npmName": "redux-core-reducers",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
